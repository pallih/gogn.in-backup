	function Getnames(filename)
{
    $.getJSON(filename, function(data){
	var names = [];
	$.each(data, function(i,s){
            names.push(s.name);
        });
    var numbers = [];
	$.each(data, function(i,s){
            numbers.push(s.nr);
        });
        names.sort();
        numbers.sort();
        $( "#nafn" ).autocomplete({
			source: names, minLength:1
		});
        $( "#nr" ).autocomplete({
			source: numbers, minLength:1
		});
    });
}

